{
  "name": "react-d3-charts",
  "version": "1.0.0",
  "description": "Implementation of Charts with React & D3",
  "main": "index.js",
  "scripts": {
    "clear": "rm -rf ./build",
    "start": "node server.js",
    "build": "npm run clear && webpack --mode production",
    "dev": "nodemon --watch *.js --exec \"webpack-dev-server --mode development\"",
    "lint": "eslint . --ext .js, .jsx",
    "lint:fix": "eslint --fix . --ext .js, .jsx",
    "heroku-prebuild": "npm install --only=dev",
    "postinstall": "webpack --mode production",
    "prepush": "npm run test",
    "test": "jest ./__tests__/test-charts",
    "test:cov": "rm -rf ./coverage && npm run test && cat ./coverage/lcov.info | coveralls"
  },
  "engines": {
    "node": "10.5.0",
    "npm": "6.1.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kukiron/react-d3-charts.git"
  },
  "greenkeeper": {
    "ignore": [
      "package-names",
      "you-want-me-to-ignore"
    ]
  },
  "keywords": [],
  "author": "Kafil Uddin",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint-config-latest": "^1.0.6",
    "husky": "^0.14.3",
    "jest-cli": "^23.4.1",
    "nodemon": "^1.18.2",
    "react-test-renderer": "^16.4.1",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "d3-axis": "^1.0.8",
    "d3-scale": "^2.1.0",
    "d3-selection": "^1.3.0",
    "d3-shape": "^1.2.0",
    "express": "^4.16.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-tabs": "^2.2.2"
  }
}
